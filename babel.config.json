{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "exclude": ["@babel/plugin-transform-classes", "@babel/plugin-transform-spread"],
        "bugfixes": true
      }
    ],
    "@babel/preset-typescript",
    [
      "minify",
      {
        "keepFnName": true,
        "keepClassName": true
      }
    ]
  ],
  "generatorOpts": {
    "importAttributesKeyword": "with"
  },
  "exclude": ["**/node_modules/**"]
}
